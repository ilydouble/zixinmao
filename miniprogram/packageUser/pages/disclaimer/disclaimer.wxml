<!--disclaimer.wxml-->
<view class="container">
  <view class="agreement-content">
    <view class="agreement-title">{{title}}</view>

    <view class="content-section" wx:for="{{sections}}" wx:key="index">
      <view class="section-title">{{item.title}}</view>
      <view class="section-content">
        <view class="content-text" wx:for="{{item.paragraphs}}" wx:key="index" wx:for-item="paragraph">
          <text>{{paragraph}}</text>
        </view>
      </view>
    </view>
  </view>
</view>

